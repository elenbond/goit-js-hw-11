{{#each this}}
<div class="photo-card">
    <a href={{largeImageURL}} class = "gallery__link">
      <img src={{webformatURL}} alt={{tags}} loading="lazy" class = "gallery__image"/>
    </a>
    <div class="info">
      <p class="info-item">
        <b>Likes</b>
        <span>{{likes}}</span>
      </p>
      <p class="info-item">
        <b>Views</b>
        <span>{{views}}</span>
      </p>
      <p class="info-item">
        <b>Comments</b>
        <span>{{comments}}</span>
      </p>
      <p class="info-item">
        <b>Downloads</b>
        <span>{{downloads}}</span>
      </p>
    </div>
</div>
{{/each}}